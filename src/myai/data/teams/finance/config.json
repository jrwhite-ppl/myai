{
  "name": "Finance Team",
  "description": "Financial strategy, tax, grants, and fundraising",
  "leader": "finance_team_leader",
  "agents": [
    "finance_team_leader",
    "chief_financial_officer",
    "finance_specialist"
  ],
  "workflows": {
    "financial_planning": "agent_os/workflows/finance_workflows.md#financial-planning-and-analysis-workflow",
    "fundraising": "agent_os/workflows/finance_workflows.md#fundraising-workflow",
    "tax_planning": "agent_os/workflows/finance_workflows.md#tax-planning-workflow",
    "grant_management": "agent_os/workflows/finance_workflows.md#grant-management-workflow"
