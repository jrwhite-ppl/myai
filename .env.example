# MyAI Environment Variables

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
MYAI_LOG_LEVEL=INFO

# Configuration paths
MYAI_CONFIG_PATH=~/.myai/config
MYAI_AGENT_PATH=~/.myai/agents

# Tool integration settings
CLAUDE_CONFIG_PATH=~/.claude
CURSOR_CONFIG_PATH=~/.cursor

# Security settings
MYAI_KEYRING_SERVICE=myai
MYAI_ENABLE_AUDIT_LOG=true

# Performance settings
MYAI_CACHE_ENABLED=true
MYAI_CACHE_TTL=3600

# Development settings
MYAI_DEBUG=false
MYAI_MOCK_INTEGRATIONS=false
